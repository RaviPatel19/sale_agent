          <field name="model">res.config.settings</field>
          <field name="inherit_id" ref="base.res_config_settings_view_form"/>
          <field name="arch" type="xml">
               <div id="proforma_configuration" position="after">
                    <div class="col-12 col-lg-6 o_setting_box" id="sale_agent_configuration">
                         <div class="content-group">
                              <div class="mt16">
                                   <label for="agent_commision_product_id" class="o_light_label"/>
